import java.util.*;

class Test
{
    int x,y;
    
    static void area(Test o1)
    {
        System.out.println("Area of rectangle = "+ (o1.x * o1.y) );
    } 

   

    Test(int x,int y)
    {
        this.x=x;
        this.y=y;
    }

    public static void main(String args[])
    {
        Test o1=new Test(20,10);
        area(o1);
    }
}
